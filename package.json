{
  "name": "dice",
  "version": "0.17.0",
  "description": "Web-based dice rolling app supporting 5 different dice types including D6, D8, D10, D12 and D20",
  "private": true,
  "keywords": [
    "dice",
    "3d",
    "d6",
    "d8",
    "d10",
    "d12",
    "d20"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf public",
    "build": "npm run clean && NODE_ENV=production webpack --colors --profile --config config/build.conf.js --display-error-details",
    "test": "echo \"No tests yet\"",
    "dev": "NODE_ENV=development webpack-serve --host 0.0.0.0 --port 8080 --config config/build.conf.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint . -f node_modules/eslint-friendly-formatter --ignore-path .gitignore --ext .jsx --ext .js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint \"src/**/*.css\" \"src/**/*.pcss\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "cannon": "^0.6.2",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "debug": "^4.1.1",
    "enhanced-resolve": "^4.1.0",
    "eslint": "^6.2.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "hammerjs": "^2.0.8",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-calc": "^7.0.1",
    "postcss-hexrgba": "^1.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postlude": "^0.5.0",
    "precss": "^4.0.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^3.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "rimraf": "^3.0.0",
    "shake.js": "^1.2.2",
    "spase": "^2.1.0",
    "style-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "three": "^0.108.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-serve": "^1.0.3"
  }
}

{
  "name": "dice",
  "version": "0.13.0",
  "description": "Web-based dice rolling app supporting 5 different dice types including D6, D8, D10, D12 and D20",
  "private": true,
  "keywords": [
    "dice",
    "3d",
    "d6",
    "d8",
    "d10",
    "d12",
    "d20"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf public",
    "build": "npm run clean && NODE_ENV=production webpack --colors --profile --config config/build.conf.js --display-error-details",
    "test": "echo \"No tests yet\"",
    "dev": "NODE_ENV=development webpack-serve --open --host 0.0.0.0 --config config/build.conf.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint . -f node_modules/eslint-friendly-formatter --ignore-path .gitignore --ext .jsx --ext .js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint \"src/**/*.css\" \"src/**/*.pcss\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cannon": "^0.6.2",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "debug": "^3.1.0",
    "enhanced-resolve": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "^1.1.11",
    "hammerjs": "^2.0.8",
    "html-webpack-plugin": "^3.2.0",
    "i18next": "^11.3.2",
    "i18next-browser-languagedetector": "^2.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-calc": "^6.0.1",
    "postcss-hexrgba": "^1.0.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postlude": "^0.4.1",
    "precss": "^3.1.2",
    "prop-types": "^15.6.1",
    "raw-loader": "^0.5.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-i18next": "^7.7.0",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "rimraf": "^2.6.2",
    "shake.js": "^1.2.2",
    "spase": "^0.5.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-css-modules": "^1.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "three": "^0.93.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.11.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.3",
    "webpack-serve": "^1.0.3"
  }
}

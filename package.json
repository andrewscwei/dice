{
  "name": "dice",
  "version": "0.21.0",
  "description": "Web-based dice rolling app supporting 5 different dice types including D6, D8, D10, D12 and D20",
  "private": true,
  "keywords": [
    "dice",
    "3d",
    "d6",
    "d8",
    "d10",
    "d12",
    "d20"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf public",
    "build": "npm run clean && NODE_ENV=production webpack --colors --profile --config config/build.conf.js --display-error-details",
    "test": "echo \"No tests yet\"",
    "dev": "NODE_ENV=development webpack-serve --host 0.0.0.0 --port 8080 --config config/build.conf.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint . -f node_modules/eslint-friendly-formatter --ignore-path .gitignore --ext .jsx --ext .js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint \"src/**/*.css\" \"src/**/*.pcss\"",
    "lint:css:fix": "npm run lint:css -- --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@babel/runtime": "^7.8.7",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "cannon": "^0.6.2",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "debug": "^4.1.1",
    "enhanced-resolve": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^5.1.0",
    "hammerjs": "^2.0.8",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-calc": "^7.0.2",
    "postcss-hexrgba": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postlude": "^0.6.0",
    "precss": "^4.0.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.2",
    "shake.js": "^1.2.2",
    "spase": "^3.21.0",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^1.2.3",
    "three": "^0.114.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-serve": "^1.0.3"
  },
  "dependencies": {}
}
